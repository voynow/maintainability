{
    "version": 2,
    "name": "maintainability",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "ls && cwd"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "main.py"
        }
    ]
}