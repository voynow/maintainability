{
    "version": 2,
    "name": "maintainability",
    "builds": [
        {
            "src": "/src/main.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "pip install poetry && poetry install"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.py"
        }
    ]
}