{
    "version": 2,
    "routes": [
        {
            "src": "/landing_page.png",
            "dest": "/landing_page.png",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            }
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}